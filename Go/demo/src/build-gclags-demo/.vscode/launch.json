    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug: Normal",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "host": "127.0.0.1",
            "port": 2345,
            "program": "${workspaceRoot}",
            "remotePath": "",
            "showLog": true,
            "showGlobalVariables": true
        },
        {
            "name": "Debug: Non-Optimized",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "host": "127.0.0.1",
            "port": 2345,
            "program": "${workspaceRoot}",
            "remotePath": "",
            "showLog": true,
            "showGlobalVariables": true,
            "args": ["-gcflags=all=\"-N -L\""]
        }
    ]
}