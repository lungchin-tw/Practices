{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "chalice deploy",
      "type": "shell",
      "command": "pipenv",
      "args": [
        "run",
        "chalice",
        "--debug",
        "deploy",
        "--profile",
        "jacky",
        "|",
        "tee",
        "deploy.log"
      ]
    },
    {
      "label": "chalice delete",
      "type": "shell",
      "command": "pipenv",
      "args": [
        "run",
        "chalice",
        "--debug",
        "delete",
        "--profile",
        "jacky",
        "|",
        "tee",
        "delete.log"
      ]
    },
    {
      "label": "run chalice local",
      "type": "shell",
      "command": "pipenv",
      "args": ["run", "chalice", "--debug", "local", "|", "tee", "local.log"]
    }
  ]
}
